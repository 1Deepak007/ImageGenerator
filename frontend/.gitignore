# Ignore node_modules directory in both frontend and backend
frontend/node_modules/
backend/node_modules/

# Ignore any build output directories
frontend/build/
frontend/dist/
backend/build/
backend/dist/

# Ignore environment variables or local configuration files
*.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore npm's log files and debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE or editor-specific files
.vscode/
.idea/

# Ignore macOS specific files
.DS_Store

# Ignore build output from Webpack
/build/

# Ignore dependencies installed via pip
/venv/
/.venv/

# Ignore SQLite database files
*.sqlite3

# Ignore logs
*.log

# Ignore compiled Python bytecode
__pycache__/

# Ignore files generated by coverage.py
.coverage

# Ignore Python environment
/.python-version
